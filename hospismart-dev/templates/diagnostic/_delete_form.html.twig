<form method="post" action="{{ path('app_diagnostic_delete', {id: diagnostic.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce diagnostic ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ diagnostic.id) }}">
    <button class="btn btn-danger">
        <i class="fas fa-trash"></i> Supprimer
    </button>
</form>
